{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended",
        ":configMigration",
        ":pinDevDependencies"
    ],
    "prHourlyLimit": 0,
    "prConcurrentLimit": 0,
    "enabledManagers": [
        "pep621",
        "github-actions",
        "git-submodules"
    ],
    "git-submodules": {
        "enabled": true
    }
}